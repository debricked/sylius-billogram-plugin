{
    "name": "debricked/sylius-billogram-plugin",
    "type": "sylius-plugin",
    "description": "Billogram payment plugin for Sylius applications.",
    "license": "MIT",
    "require": {
        "php": "^7.2",

        "sylius/sylius": "^1.2",
        "friendsofapi/billogram": "dev-master#80ce3438c4f4b980aa255793725aeec3dfd059c1"
    },
    "authors": [
        {
            "name": "debricked AB",
            "homepage": "https://debricked.com"
        },
        {
            "name": "Oscar Reimer",
            "email": "oscar.reimer@debricked.com"
        }
    ],
    "require-dev": {
        "behat/behat": "^3.3",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "friends-of-behat/context-service-extension": "^1.0",
        "friends-of-behat/cross-container-extension": "^1.0",
        "friends-of-behat/service-container-extension": "^1.0",
        "friends-of-behat/symfony-extension": "^1.0",
        "friends-of-behat/variadic-extension": "^1.0",
        "lakion/mink-debug-extension": "^1.2.3",
        "phpspec/phpspec": "^5.0",
        "phpstan/phpstan-shim": "^0.9.2",
        "phpunit/phpunit": "^6.5",
        "se/selenium-server-standalone": "^2.52",
        "sylius-labs/coding-standard": "^2.0",
        "symplify/easy-coding-standard": "^4.6",
        "nelmio/alice": "^3.3"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Debricked\\SyliusBillogramPlugin\\": "src/",
            "Tests\\Debricked\\SyliusBillogramPlugin\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/Application/app/AppKernel.php"]
    },
    "config": {
        "bin-dir": "bin"
    }
}
